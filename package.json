    "devDependencies": {
        "@changesets/cli": "^2.4.0",
        "@types/jest": "^24.0.23",
        "@types/node": "^12.12.8",
        "@typescript-eslint/eslint-plugin": "^2.7.0",
        "@typescript-eslint/parser": "^2.7.0",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.6.0",
        "eslint-config-wanews-base": "^2.0.2",
        "jest": "^24.9.0",
        "node-knex-query-executor": "^2.0.1",
